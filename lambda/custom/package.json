    "i18next": "^15.0.5",
    "i18next-sprintf-postprocessor": "^0.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/i18next-sprintf-postprocessor": "^0.2.0",
    "@types/mocha": "^8.0.1",
    "@types/node": "^10.10.0",
    "chai": "^4.2.0",
    "cpy-cli": "^3.1.0",
    "gts": "^1.1.2",
